{
  "name": "rest-express",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:server": "tsx server/server.ts",
    "start:server": "tsx server/index.ts",
    "ws:dev": "tsx server/ws.ts",
    "ws:start": "node server/websocket-server.js",
    "db:migrate": "drizzle-kit migrate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint:fix",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest --run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next dist out",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run type-check && npm run lint:fix && npm run test:ci",
    "build:analyze": "ANALYZE=true next build",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "db:studio": "drizzle-kit studio",
    "benchmark": "node scripts/benchmark.js",
    "bundle:size": "npm run build && npx bundlesize",
    "perf:measure": "node scripts/performance-measure.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:desktop": "playwright test --project='Desktop Chrome' --project='Desktop Firefox' --project='Desktop Safari'",
    "test:e2e:tablet": "playwright test --project='Tablet'",
    "test:e2e:performance": "playwright test --project='Performance Tests'",
    "test:e2e:accessibility": "playwright test --project='Accessibility Tests'",
    "test:e2e:visual": "playwright test --project='Visual Tests'",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:load": "artillery run artillery.yml",
    "test:load:light": "artillery run tests/load/load-test.js --config '{\"target\": \"http://localhost:3000\", \"phases\": [{\"duration\": \"2m\", \"arrivalRate\": 5}]}'",
    "test:load:medium": "artillery run tests/load/load-test.js --config '{\"target\": \"http://localhost:3000\", \"phases\": [{\"duration\": \"5m\", \"arrivalRate\": 25}]}'",
    "test:load:heavy": "artillery run tests/load/load-test.js --config '{\"target\": \"http://localhost:3000\", \"phases\": [{\"duration\": \"10m\", \"arrivalRate\": 100}]}'",
    "test:load:spike": "artillery run tests/load/load-test.js --config '{\"target\": \"http://localhost:3000\", \"phases\": [{\"duration\": \"1m\", \"arrivalRate\": 200}]}'",
    "test:all": "npm run test && npm run test:e2e && npm run test:load:light",
    "docker:build": "docker build -t cryptotrader-app .",
    "docker:run": "docker run -p 3000:3000 cryptotrader-app",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:build": "docker-compose build",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:compose:production": "docker-compose --profile production up -d",
    "docker:compose:monitoring": "docker-compose --profile monitoring up -d",
    "deploy:staging": "echo 'Deploying to staging...' && npm run docker:compose:up",
    "deploy:production": "echo 'Deploying to production...' && npm run docker:compose:production",
    "setup:dev": "npm install && npm run db:generate && npm run db:push",
    "setup:test": "npm install && npx playwright install",
    "ci:setup": "npm ci && npx playwright install --with-deps",
    "ci:test": "npm run validate && npm run test:e2e && npm run test:load:light",
    "precommit": "lint-staged && npm run type-check"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
